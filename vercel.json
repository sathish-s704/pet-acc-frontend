{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://pet-acc-backend.onrender.com/api/$1"
        },
        { "handle": "filesystem" },
        { "src": "/.*", "dest": "/index.html" }
    ]
}